package client;
import java.util.*;
import service.DataService;
import service.DataService_Service;
public class CharacterClientMain {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "pjw616US";
            DataService_Service service = new DataService_Service();
            DataService port = service.getDataServicePort();
            List<Integer> numbers = port.getData(studentCode, qCode);
            System.out.println("== Danh s√°ch s·ªë nguy√™n nh·∫≠n t·ª´ server ==");
            System.out.println(numbers);
            List<String> binaries = new ArrayList<>();
            for (Integer num : numbers) {
                binaries.add(Integer.toBinaryString(num));
            }
            System.out.println("\n== Chu·ªói nh·ªã ph√¢n ==");
            System.out.println(binaries);
            port.submitDataStringArray(studentCode, qCode, binaries);
            System.out.println("\n== ƒê√£ g·ª≠i d·ªØ li·ªáu nh·ªã ph√¢n v·ªÅ server th√†nh c√¥ng! ==");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}


data-ws: chuy·ªÉn h·ªá 10->8 v√† 16
package client;
import java.util.*;
import service.DataService;
import service.DataService_Service;
public class DataClientMain {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "zBC7JEDm";
            DataService_Service service = new DataService_Service();
            DataService port = service.getDataServicePort();
            List<Integer> numbers = port.getData(studentCode, qCode);
            System.out.println("üì• D·ªØ li·ªáu nh·∫≠n t·ª´ server:");
            System.out.println(numbers);
            List<String> resultList = new ArrayList<>();
            for (Integer num : numbers) {
                if (num == null) continue;
                String base8 = Integer.toOctalString(num);
                String base16 = Integer.toHexString(num).toUpperCase();
                String combined = base8 + "|" + base16;
                resultList.add(combined);
            }
            System.out.println("üì§ K·∫øt qu·∫£ sau chuy·ªÉn ƒë·ªïi:");
            for (String s : resultList) {
                System.out.println(s);
            }
            port.submitDataStringArray(studentCode, qCode, resultList);
            System.out.println("‚úÖ ƒê√£ g·ª≠i danh s√°ch k·∫øt qu·∫£ v·ªÅ server th√†nh c√¥ng.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}



2) Object-ws: s·∫Øp x·∫øp nh√¢n vi√™n th√¢m ni√™n cao th·∫•p 
package client;
import java.util.*;
import service.EmployeeY;
import service.ObjectService;
import service.ObjectService_Service;
public class CharacterClientMain {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "UKhSGH37";
            ObjectService_Service service = new ObjectService_Service();
            ObjectService port = service.getObjectServicePort();
            List<EmployeeY> employees = port.requestListEmployeeY(studentCode, qCode);
            System.out.println("=== Danh s√°ch nh·∫≠n t·ª´ server ===");
            for (EmployeeY e : employees) {
                System.out.println(e.getName() + " - " + e.getStartDate());
            }
            employees.sort(Comparator.comparing(e -> e.getStartDate().toGregorianCalendar().getTime()));
            System.out.println("\n=== Danh s√°ch sau khi s·∫Øp x·∫øp ===");
            for (EmployeeY e : employees) {
                System.out.println(e.getName() + " - " + e.getStartDate());
            }
            port.submitListEmployeeY(studentCode, qCode, employees);
            System.out.println("\n‚úÖ Danh s√°ch ƒë√£ ƒë∆∞·ª£c g·ª≠i l·∫°i server th√†nh c√¥ng!");
        } catch (Exception ex) {
            ex.printStackTrace();
        }
    }
}


Object-ws: l·ªçc gi·ªØ sinh vi√™n
package client;
import java.util.*;
import service.ObjectService;
import service.ObjectService_Service;
import service.Student;
public class CharacterClientMain {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN021";
            String qCode = "SEg1fN6N";
            ObjectService_Service service = new ObjectService_Service();
            ObjectService port = service.getObjectServicePort();
            List<Student> students = port.requestListStudent(studentCode, qCode);
            System.out.println("üì• Danh s√°ch sinh vi√™n nh·∫≠n t·ª´ server:");
            for (Student s : students) {
                System.out.println(s.getName() + " - " + s.getScore());
            }
            List<Student> filtered = new ArrayList<>();
            for (Student s : students) {
                float score = s.getScore();
                if (score >= 8.0 || score < 5.0) { // nh√≥m A ho·∫∑c D
                    filtered.add(s);
                }
            }
            System.out.println("\nüì§ Danh s√°ch sinh vi√™n nh√≥m A v√† D:");
            for (Student s : filtered) {
                System.out.println(s.getName() + " - " + s.getScore());
            }
            port.submitListStudent(studentCode, qCode, filtered);
            System.out.println("\n‚úÖ ƒê√£ g·ª≠i danh s√°ch sinh vi√™n nh√≥m A v√† D v·ªÅ server.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}



3) Character-ws: c√πng s·ªë nguy√™n √¢m:
package client;
import java.util.*;
import service.CharacterService;
import service.CharacterService_Service;
public class CharacterClientMain {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "xnVPNkXX";
            CharacterService_Service service = new CharacterService_Service();
            CharacterService port = service.getCharacterServicePort();
            List<String> words = port.requestStringArray(studentCode, qCode);
            System.out.println("D·ªØ li·ªáu g·ªëc t·ª´ server:");
            System.out.println(words);
            Map<Integer, List<String>> groupMap = new TreeMap<>(); // nh√≥m theo s·ªë nguy√™n √¢m
            for (String word : words) {
                int vowelCount = countVowels(word);
                groupMap.putIfAbsent(vowelCount, new ArrayList<>());
                groupMap.get(vowelCount).add(word);
            }
            List<String> resultList = new ArrayList<>();
            for (Map.Entry<Integer, List<String>> entry : groupMap.entrySet()) {
                List<String> groupWords = entry.getValue();
                Collections.sort(groupWords);
                String groupString = String.join(", ", groupWords);
                resultList.add(groupString);
            }
            System.out.println("K·∫øt qu·∫£ nh√≥m sau x·ª≠ l√Ω:");
            for (String s : resultList) {
                System.out.println(s);
            }
            port.submitCharacterStringArray(studentCode, qCode, resultList);
            System.out.println("‚úÖ ƒê√£ g·ª≠i d·ªØ li·ªáu k·∫øt qu·∫£ v·ªÅ server.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
    private static int countVowels(String word) {
        int count = 0;
        String vowels = "aeiouAEIOU";
        for (char c : word.toCharArray()) {
            if (vowels.indexOf(c) != -1)
                count++;
        }
        return count;
    }
}


Character-ws: chuy·ªÉn chu·ªói th√†nh 3 ƒë·ªãnh d·∫°ng
package client;
import java.util.*;
import service.CharacterService;
import service.CharacterService_Service;
public class CharacterClientMain {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN021";
            String qCode = "x4R8JP2B";
            CharacterService_Service service = new CharacterService_Service();
            CharacterService port = service.getCharacterServicePort();
            String original = port.requestString(studentCode, qCode);
            System.out.println("Chu·ªói g·ªëc nh·∫≠n t·ª´ server:");
            System.out.println(original);
            String[] words = original.trim().split("[ _]+");
            String pascalCase = toPascalCase(words);
            String camelCase = toCamelCase(words);
            String snakeCase = toSnakeCase(words);
            System.out.println("K·∫øt qu·∫£ chuy·ªÉn ƒë·ªïi:");
            System.out.println("PascalCase: " + pascalCase);
            System.out.println("camelCase: " + camelCase);
            System.out.println("snake_case: " + snakeCase);
            List<String> resultList = Arrays.asList(pascalCase, camelCase, snakeCase);
            port.submitCharacterStringArray(studentCode, qCode, resultList);
            System.out.println("‚úÖ ƒê√£ g·ª≠i k·∫øt qu·∫£ v·ªÅ server th√†nh c√¥ng.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
    private static String toPascalCase(String[] words) {
        StringBuilder sb = new StringBuilder();
        for (String w : words) {
            if (!w.isEmpty()) {
                sb.append(Character.toUpperCase(w.charAt(0)))
                  .append(w.substring(1).toLowerCase());
            }
        }
        return sb.toString();
    }
    private static String toCamelCase(String[] words) {
        if (words.length == 0) return "";
        StringBuilder sb = new StringBuilder(words[0].toLowerCase());
        for (int i = 1; i < words.length; i++) {
            String w = words[i];
            if (!w.isEmpty()) {
                sb.append(Character.toUpperCase(w.charAt(0)))
                  .append(w.substring(1).toLowerCase());
            }
        }
        return sb.toString();
    }
    private static String toSnakeCase(String[] words) {
        List<String> lowerWords = new ArrayList<>();
        for (String w : words) {
            if (!w.isEmpty()) {
                lowerWords.add(w.toLowerCase());
            }
        }
        return String.join("_", lowerWords);
    }
}



4) RMI-byte: 2->16
package client;
import RMI.ByteService;
import java.rmi.Naming;
import java.nio.charset.StandardCharsets;
public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "CKjwhuhk";
            String rmiUrl = "rmi://203.162.10.109/RMIByteService";
            System.out.println("K·∫øt n·ªëi t·ªõi RMI Server...");
            ByteService service = (ByteService) Naming.lookup(rmiUrl);
            System.out.println("Nh·∫≠n d·ªØ li·ªáu nh·ªã ph√¢n t·ª´ server...");
            byte[] data = service.requestData(studentCode, qCode);
            System.out.println("D·ªØ li·ªáu nh·ªã ph√¢n nh·∫≠n ƒë∆∞·ª£c (" + data.length + " byte):");
            for (byte b : data) {
                System.out.print((b & 0xFF) + " ");
            }
            System.out.println();
            String hexString = bytesToHex(data);
            System.out.println("Chu·ªói HEX t∆∞∆°ng ·ª©ng: " + hexString);
            byte[] hexAsciiBytes = hexString.getBytes(StandardCharsets.US_ASCII);
            System.out.print("M·∫£ng byte g·ª≠i ƒëi: [");
            for (int i = 0; i < hexAsciiBytes.length; i++) {
                System.out.print((hexAsciiBytes[i] & 0xFF));
                if (i < hexAsciiBytes.length - 1) System.out.print(", ");
            }
            System.out.println("]");
            service.submitData(studentCode, qCode, hexAsciiBytes);
            System.out.println("ƒê√£ g·ª≠i d·ªØ li·ªáu HEX (ASCII) tr·ªü l·∫°i server.");
            System.out.println("Ho√†n t·∫•t. K·∫øt th√∫c ch∆∞∆°ng tr√¨nh client.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
    private static String bytesToHex(byte[] bytes) {
        StringBuilder sb = new StringBuilder();
        for (byte b : bytes) {
            sb.append(String.format("%02x", b)); // m·ªói byte -> 2 k√Ω t·ª± hex
        }
        return sb.toString();
    }
}


RMI-byte: d·ªãch byte m·∫£ng nh·ªã ph√¢n:
package Client;
import RMI.ByteService;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
import java.util.Arrays;
public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "kpNlrfB9";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109", 1099);
            ByteService service = (ByteService) registry.lookup("RMIByteService");
            byte[] data = service.requestData(studentCode, qCode);
            System.out.println("D·ªØ li·ªáu nh·∫≠n t·ª´ server (byte[]): " + Arrays.toString(data));
            String input = new String(data, "ASCII");
            System.out.println("D·ªØ li·ªáu ASCII: " + input);
            byte[] encoded = caesarEncode(data);
            String encodedText = new String(encoded, "ASCII");
            System.out.println("Sau khi m√£ h√≥a Caesar: " + encodedText);
            System.out.println("M·∫£ng byte sau m√£ h√≥a: " + Arrays.toString(encoded));
            service.submitData(studentCode, qCode, encoded);
            System.out.println("ƒê√£ g·ª≠i d·ªØ li·ªáu m√£ h√≥a v·ªÅ server. K·∫øt th√∫c ch∆∞∆°ng tr√¨nh.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
    private static byte[] caesarEncode(byte[] data) {
        int shift = data.length;
        byte[] result = new byte[data.length];
        for (int i = 0; i < data.length; i++) {
            int val = (data[i] & 0xFF);
            val = (val + shift) % 256;
            result[i] = (byte) val;
        }
        return result;
    }
}


RMI-byte: chia m·∫£nh ch·∫µn l·∫ª:
package Client;
import RMI.ByteService;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
import java.util.ArrayList;
import java.util.List;
public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129"; 
            String qCode = "CCJECxDx";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109", 1099);
            ByteService service = (ByteService) registry.lookup("RMIByteService");
            byte[] data = service.requestData(studentCode, qCode);
            System.out.print("D·ªØ li·ªáu g·ªëc nh·∫≠n t·ª´ server: ");
            printArray(data);
            List<Byte> evenList = new ArrayList<>();
            List<Byte> oddList = new ArrayList<>();
            for (byte b : data) {
                if (b % 2 == 0)
                    evenList.add(b);
                else
                    oddList.add(b);
            }
            List<Byte> resultList = new ArrayList<>();
            resultList.addAll(evenList);
            resultList.addAll(oddList);
            byte[] result = new byte[resultList.size()];
            for (int i = 0; i < resultList.size(); i++) {
                result[i] = resultList.get(i);
            }
            System.out.print("M·∫£ng sau khi s·∫Øp x·∫øp ch·∫µn-l·∫ª: ");
            printArray(result);
            service.submitData(studentCode, qCode, result);
            System.out.println("‚úÖ ƒê√£ g·ª≠i d·ªØ li·ªáu byte[] ƒë√£ x·ª≠ l√Ω v·ªÅ server th√†nh c√¥ng!");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
    private static void printArray(byte[] arr) {
        System.out.print("[");
        for (int i = 0; i < arr.length; i++) {
            System.out.print(arr[i]);
            if (i < arr.length - 1) System.out.print(", ");
        }
        System.out.println("]");
    }
}


RMI-byte: XOR:
package Client;
import RMI.ByteService;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "wkR7Vk9q";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109", 1099);
            ByteService service = (ByteService) registry.lookup("RMIByteService");
            byte[] data = service.requestData(studentCode, qCode);
            System.out.print("D·ªØ li·ªáu nh·∫≠n t·ª´ server (ASCII): ");
            System.out.println(new String(data));
            String key = "PTIT";
            byte[] keyBytes = key.getBytes();
            byte[] encrypted = new byte[data.length];
            for (int i = 0; i < data.length; i++) {
                encrypted[i] = (byte) (data[i] ^ keyBytes[i % keyBytes.length]);
            }
            System.out.print("D·ªØ li·ªáu sau khi m√£ h√≥a XOR: ");
            for (byte b : encrypted) {
                System.out.print(b + " ");
            }
            System.out.println();
            service.submitData(studentCode, qCode, encrypted);
            System.out.println("‚úÖ ƒê√£ g·ª≠i d·ªØ li·ªáu XOR m√£ h√≥a v·ªÅ server th√†nh c√¥ng!");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}




5) RMI-Character: ƒë·∫øm t·∫ßn s·ªë k√Ω t·ª± 
package Client;
import RMI.CharacterService;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
import java.util.LinkedHashMap;
import java.util.Map;
public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "D0jvkPrk";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109");
            CharacterService service = (CharacterService) registry.lookup("RMICharacterService");
            String input = service.requestCharacter(studentCode, qCode);
            System.out.println("Chu·ªói nh·∫≠n t·ª´ server: " + input);
            Map<Character, Integer> freq = new LinkedHashMap<>();
            for (char c : input.toCharArray()) {
                freq.put(c, freq.getOrDefault(c, 0) + 1);
            }
            StringBuilder result = new StringBuilder();
            result.append("{");
            boolean first = true;
            for (Map.Entry<Character, Integer> e : freq.entrySet()) {
                if (!first) result.append(", ");
                result.append("\"").append(e.getKey()).append("\": ").append(e.getValue());
                first = false;
            }
            result.append("}");
            System.out.println("K·∫øt qu·∫£ ƒë·∫øm k√Ω t·ª±: " + result);
            service.submitCharacter(studentCode, qCode, result.toString());
            System.out.println("ƒê√£ g·ª≠i k·∫øt qu·∫£ v·ªÅ server. K·∫øt th√∫c ch∆∞∆°ng tr√¨nh.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}


RMI-Character: <k√Ω t·ª±><s·ªë l·∫ßn xu·∫•t hi·ªán>:
package Client;
import RMI.CharacterService;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
import java.util.LinkedHashMap;
import java.util.Map;
public class RMIClient{
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "FirrXr9v";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109", 1099);
            CharacterService service = (CharacterService) registry.lookup("RMICharacterService");
            String input = service.requestCharacter(studentCode, qCode);
            System.out.println("Chu·ªói nh·∫≠n t·ª´ server: " + input);
            Map<Character, Integer> freqMap = new LinkedHashMap<>();
            for (char c : input.toCharArray()) {
                freqMap.put(c, freqMap.getOrDefault(c, 0) + 1);
            }
            StringBuilder result = new StringBuilder();
            for (Map.Entry<Character, Integer> entry : freqMap.entrySet()) {
                result.append(entry.getKey()).append(entry.getValue());
            }
            System.out.println("Chu·ªói k·∫øt qu·∫£ sau x·ª≠ l√Ω: " + result);
            service.submitCharacter(studentCode, qCode, result.toString());
            System.out.println("‚úÖ ƒê√£ g·ª≠i chu·ªói k·∫øt qu·∫£ v·ªÅ server th√†nh c√¥ng!");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}




6) RMI-Data: sinh t·ªï h·ª£p k·∫ø ti·∫øp:
package Client;
import RMI.DataService;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "YOUR_STUDENT_CODE";
            String qCode = "A7AatfYv";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109", 1099);
            DataService service = (DataService) registry.lookup("RMIDataService");
            Object data = service.requestData(studentCode, qCode);
            int[] arr = parseData(data);
            System.out.println("Chu·ªói nh·∫≠n t·ª´ server: " + joinArray(arr));
            nextPermutation(arr);
            String result = joinArray(arr);
            System.out.println("T·ªï h·ª£p k·∫ø ti·∫øp: " + result);
            service.submitData(studentCode, qCode, result);
            System.out.println("ƒê√£ g·ª≠i k·∫øt qu·∫£ v·ªÅ server. K·∫øt th√∫c ch∆∞∆°ng tr√¨nh.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }

    private static int[] parseData(Object data) {
        if (data instanceof int[]) {
            return (int[]) data;
        } else if (data instanceof String) {
            String str = ((String) data).replaceAll("[^0-9,]", "");
            String[] parts = str.split(",");
            int[] arr = new int[parts.length];
            for (int i = 0; i < parts.length; i++) {
                arr[i] = Integer.parseInt(parts[i].trim());
            }
            return arr;
        } else {
            throw new IllegalArgumentException("D·ªØ li·ªáu kh√¥ng h·ª£p l·ªá t·ª´ server!");
        }
    }

    private static String joinArray(int[] arr) {
        StringBuilder sb = new StringBuilder();
        for (int i = 0; i < arr.length; i++) {
            sb.append(arr[i]);
            if (i < arr.length - 1) sb.append(",");
        }
        return sb.toString();
    }
    private static void nextPermutation(int[] arr) {
        int n = arr.length;
        int i = n - 2;
        while (i >= 0 && arr[i] >= arr[i + 1]) i--;
        if (i >= 0) {
            int j = n - 1;
            while (arr[j] <= arr[i]) j--;
            swap(arr, i, j);
        }
        reverse(arr, i + 1, n - 1);
    }
    private static void swap(int[] arr, int i, int j) {
        int tmp = arr[i];
        arr[i] = arr[j];
        arr[j] = tmp;
    }
    private static void reverse(int[] arr, int l, int r) {
        while (l < r) swap(arr, l++, r--);
    }
}


RMI-data: x·∫øp ƒë·ªìng xu:
package Client;
import RMI.DataService;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
import java.util.ArrayList;
import java.util.List;

public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN021";
            String qCode = "zEYyMBr6";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109", 1099);
            DataService service = (DataService) registry.lookup("RMIDataService");
            Object received = service.requestData(studentCode, qCode);
            int amount = (int) received;
            System.out.println("S·ªë ti·ªÅn nh·∫≠n ƒë∆∞·ª£c t·ª´ server: " + amount);
            int[] coins = {10, 5, 2, 1};
            List<Integer> usedCoins = new ArrayList<>();
            int count = 0;
            int remaining = amount;

            for (int coin : coins) {
                while (remaining >= coin) {
                    remaining -= coin;
                    usedCoins.add(coin);
                    count++;
                }
            }
            String result;
            if (remaining != 0) {
                result = "-1";
            } else {
                StringBuilder sb = new StringBuilder();
                sb.append(count).append("; ");
                for (int i = 0; i < usedCoins.size(); i++) {
                    sb.append(usedCoins.get(i));
                    if (i < usedCoins.size() - 1) sb.append(",");
                }
                result = sb.toString();
            }
            System.out.println("K·∫øt qu·∫£ c·∫ßn g·ª≠i: " + result);
            service.submitData(studentCode, qCode, result);
            System.out.println("ƒê√£ g·ª≠i k·∫øt qu·∫£ v·ªÅ server th√†nh c√¥ng.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}



7) RMI-Object: chuy·ªÉn m√£ s·∫£n ph·∫©m + c·∫≠p nh·∫≠t gi√°:
package Client;
import RMI.ObjectService;
import RMI.Product;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "2EHLi0AR";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109", 1099);
            ObjectService service = (ObjectService) registry.lookup("RMIObjectService");
            Object obj = service.requestObject(studentCode, qCode);
            if (!(obj instanceof Product)) {
                System.out.println("ƒê·ªëi t∆∞·ª£ng nh·∫≠n ƒë∆∞·ª£c kh√¥ng ph·∫£i Product!");
                return;
            }
            Product p = (Product) obj;
            System.out.println("S·∫£n ph·∫©m nh·∫≠n t·ª´ server:");
            System.out.println(p);
            p.setCode(p.getCode().toUpperCase());
            p.setExportPrice(p.getImportPrice() * 1.2);
            System.out.println("Sau khi chu·∫©n h√≥a:");
            System.out.println(p);
            service.submitObject(studentCode, qCode, p);
            System.out.println("ƒê√£ g·ª≠i ƒë·ªëi t∆∞·ª£ng ƒë√£ chu·∫©n h√≥a v·ªÅ server. K·∫øt th√∫c ch∆∞∆°ng tr√¨nh.");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}


RMI-object: t·∫°o m√£ order:
package Client;
import RMI.ObjectService;
import RMI.Order;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129";
            String qCode = "YgEnia8N";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109", 1099);
            ObjectService service = (ObjectService) registry.lookup("RMIObjectService");
            Order order = (Order) service.requestObject(studentCode, qCode);
            System.out.println("ƒê∆°n h√†ng nh·∫≠n ƒë∆∞·ª£c t·ª´ server:");
            System.out.println(order);
            String shipping = order.getShippingType();
            String customer = order.getCustomerCode();
            String date = order.getOrderDate();
            String prefix = shipping.substring(0, 2).toUpperCase();
            String suffix = customer.substring(customer.length() - 3);
            String day = date.substring(8, 10);
            String month = date.substring(5, 7);
            String orderCode = prefix + suffix + day + month;
            order.setOrderCode(orderCode);
            System.out.println("M√£ ƒë∆°n h√†ng m·ªõi: " + orderCode);
            service.submitObject(studentCode, qCode, order);
            System.out.println("ƒê√£ g·ª≠i l·∫°i ƒë·ªëi t∆∞·ª£ng Order ƒë√£ c·∫≠p nh·∫≠t th√†nh c√¥ng!");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}


RMI-object: m√£ gi·∫£m gi√°:
package Client;
import RMI.ObjectService;
import RMI.Product;
import java.rmi.registry.LocateRegistry;
import java.rmi.registry.Registry;
public class RMIClient {
    public static void main(String[] args) {
        try {
            String studentCode = "B22DCCN129"; 
            String qCode = "EyLG9oK1";
            Registry registry = LocateRegistry.getRegistry("203.162.10.109", 1099);
            ObjectService service = (ObjectService) registry.lookup("RMIObjectService");
            ProductX product = (ProductX) service.requestObject(studentCode, qCode);
            System.out.println("ƒê·ªëi t∆∞·ª£ng ProductX nh·∫≠n ƒë∆∞·ª£c t·ª´ server:");
            System.out.println("ID: " + product.getId());
            System.out.println("Code: " + product.getCode());
            System.out.println("DiscountCode: " + product.getDiscountCode());
            System.out.println("Discount ban ƒë·∫ßu: " + product.getDiscount());
            String discountCode = product.getDiscountCode();
            int sum = 0;
            for (char c : discountCode.toCharArray()) {
                if (Character.isDigit(c)) {
                    sum += Character.getNumericValue(c);
                }
            }
            product.setDiscount(sum);
            System.out.println("‚úÖ T·ªïng c√°c ch·ªØ s·ªë trong discountCode = " + sum);
            System.out.println("ƒê√£ c·∫≠p nh·∫≠t discount m·ªõi cho ProductX!");
            service.submitObject(studentCode, qCode, product);
            System.out.println("‚úÖ ƒê√£ g·ª≠i ProductX ƒë√£ c·∫≠p nh·∫≠t v·ªÅ server th√†nh c√¥ng!");
        } catch (Exception e) {
            e.printStackTrace();
        }
    }
}
